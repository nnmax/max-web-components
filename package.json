{
  "name": "max-web-component",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "repository": "https://github.com/nnmax/max-web-component.git",
  "author": "maxin <maxin@growingio.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn storybook",
    "build": "rollup --config rollup.config.js",
    "build:watch": "yarn build --watch",
    "test": "web-test-runner --node-resolve",
    "eslint": "eslint . --ext .js,.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prettier": "prettier --write .",
    "prettier:ci": "prettier --check .",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "analyze": "custom-elements-manifest analyze --config \"custom-elements-manifest.config.mjs\""
  },
  "dependencies": {
    "lit-html": "^2.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@mdx-js/react": "^1.6.22",
    "@nnmax/eslint-config-typescript": "^1.2.0",
    "@open-wc/testing": "^3.1.6",
    "@rollup/plugin-typescript": "^8.5.0",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-docs": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/web-components": "^6.5.13",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@web/dev-server-esbuild": "^0.3.3",
    "@web/test-runner": "^0.15.0",
    "acorn-import-assertions": "^1.8.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.6",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "prettier": "2.7.1",
    "rollup": "^2.79.1",
    "rollup-plugin-import-assert": "^2.1.3",
    "typescript": "^4.8.4"
  },
  "packageManager": "yarn@3.3.0",
  "lint-staged": {
    "*.(j|t)s": "eslint --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
